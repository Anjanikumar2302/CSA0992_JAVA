import java.io.*;
class matrixadd{
    public static void main(String[] args){
        int mat1[][]={{1,2},{5,3}};
        int mat2[][]={{2,3},{4,1}};
        int matsum[][]=new int[2][2];
        int len=2;
        for(int i=0;i<len;i++){
            for(int j=0;j<len;j++){
                matsum[i][j]=mat1[i][j]+mat2[i][j];
                System.out.print(matsum[i][j]+"\t");
            }
            System.out.println();
        }
    }
}
